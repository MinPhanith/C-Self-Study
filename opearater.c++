#include<iostream>
using namespace std;
int main(){
    int n1,n2;
    cout<<"Input number 1:";cin>>n1;
    cout<<"Input number 2:";cin>>n2;

    cout<<"Result:"<<n1<<"+"<<n2<<"="<<n1+n2<<endl;
    cout<<"Result:"<<n1<<"-"<<n2<<"="<<n1-n2<<endl; 
    cout<<"Result:"<<n1<<"*"<<n2<<"="<<n1*n2<<endl;
    cout<<"Result:"<<n1<<"/"<<n2<<"="<<n1/n2<<endl;


}